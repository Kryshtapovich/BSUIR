{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@Components": "./Components",
          "@Hooks": "./Hooks",
          "@Models": "./Models",
          "@Services": "./Services",
          "@Stores": "./Stores",
          "@utils": "./utils"
        }
      }
    ]
  ]
}
